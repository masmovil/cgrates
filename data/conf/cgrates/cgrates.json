  "cdrs": {
    "enabled": true,
    "attributes_conns": [{
        "address": "0.0.0.0:2012",
        "transport": "*json"
    }],
    "chargers_conns": [{
        "address": "0.0.0.0:2012",
        "transport": "*json"
    }],
    "rals_conns": [{
        "address": "0.0.0.0:2012",
        "transport": "*json"
    }]
