  "rals": {
    "enabled": true,
    "thresholds_conns": [],
    "stats_conns": [],
    "rp_subject_prefix_matching": false,
    "remove_expired":true,
    "max_computed_usage": {
      "*any": "189h",
      "*voice": "72h",
      "*data": "107374182400",
      "*sms": "10000"
    },
    "balance_rating_subject":{
      "*any": "*zero1ns",
      "*voice": "*zero1s",
      "*data": "*zero1ns",
      "*sms": "*zero1ns",
      "*monetary":"*zero1ns",
      "*generic":"*zero1ns"
    }
